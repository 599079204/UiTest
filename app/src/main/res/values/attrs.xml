<resources>

    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

    <declare-styleable name="SlidingMenu">
        <attr name="mode">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="viewAbove" format="reference" />
        <attr name="viewBehind" format="reference" />
        <attr name="behindOffset" format="dimension" />
        <attr name="behindWidth" format="dimension" />
        <attr name="behindScrollScale" format="float" />
        <attr name="touchModeAbove">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="touchModeBehind">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="shadowDrawable" format="reference" />
        <attr name="shadowWidth" format="dimension" />
        <attr name="fadeEnabled" format="boolean" />
        <attr name="fadeDegree" format="float" />
        <attr name="selectorEnabled" format="boolean" />
        <attr name="selectorDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="indicator_color" format="color" />
        <attr name="tab_text_color" format="color" />
        <attr name="underline_color" format="color" />
        <attr name="divider_color" format="color" />
        <attr name="indicator_height" format="dimension" />
        <attr name="tab_text_size" format="dimension" />
        <attr name="underline_height" format="dimension" />
        <attr name="selected_tab_text_color" format="color" />
        <attr name="divider_padding" format="dimension" />
        <attr name="divider_width" format="dimension" />
        <attr name="tab_padding_left_right" format="dimension" />
        <attr name="scroll_offset" format="dimension" />
        <attr name="tab_background" format="reference" />
        <attr name="should_match_width" format="boolean" />
        <attr name="text_all_caps" format="boolean" />
        <attr name="cursor_align_text" format="boolean" />
        <attr name="selected_text_bold" format="boolean" />
        <attr name="cursor_round_corner" format="boolean" />
        <attr name="indicator_match_height" format="boolean" />
        <attr name="underline_margin_top" format="dimension" />
        <attr name="corner_radius" format="dimension" />
        <attr name="max_line_width" format="dimension" />
        <attr name="match_width" format="enum|dimension">
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="pager_smooth_scroll" format="boolean" />
        <attr name="max_match_size" format="integer" />
    </declare-styleable>

    <declare-styleable name="RotateImageView">
        <attr name="rounded" format="boolean" />
        <attr name="interval_degree" format="float" />
        <attr name="interval_time" format="integer" />
        <attr name="during" format="integer" />
        <attr name="is_clockwise" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RoundCornerImageView">
        <attr name="all_radius" format="dimension" />
        <attr name="lt_radius" format="dimension" />
        <attr name="rt_radius" format="dimension" />
        <attr name="rb_radius" format="dimension" />
        <attr name="lb_radius" format="dimension" />
        <attr name="impl_type" format="enum">
            <enum name="clip" value="0" />
            <enum name="shader" value="1" />
            <enum name="mask" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ProgressView">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="progress" format="integer" />
    </declare-styleable>

    <declare-styleable name="KsyPlayerView">
        <!-- 自动播放 -->
        <attr name="autoPlay" format="boolean" />
        <!-- 自动循环 -->
        <attr name="loop" format="boolean" />
        <!-- 开启缓存 -->
        <attr name="cache" format="boolean" />
        <!-- 开启全屏 -->
        <attr name="fullscreen" format="boolean" />
        <!-- 是否显示控制板 -->
        <attr name="controller" format="boolean" />
        <!-- 是否显示loading图 -->
        <attr name="loading" format="boolean" />
        <!-- 缩放模式 -->
        <attr name="scaleMode" format="enum">
            <enum name="adjustSize" value="0" />
            <enum name="fitXY" value="1" />
            <enum name="adjustVideo" value="2" />
            <enum name="fitCenter" value="3" />
        </attr>
        <!-- 背景 -->
        <attr name="backDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="CircleTraceView">
        <attr name="src" format="reference|color" />
        <attr name="src_width" format="dimension" />
        <attr name="src_height" format="dimension" />
        <attr name="radius" format="dimension" />
        <attr name="speed" format="float" />
    </declare-styleable>

    <declare-styleable name="RecyclerViewWrapper">
        <attr name="scroll_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="layout_type" format="enum">
            <enum name="linear" value="0" />
            <enum name="grid" value="1" />
            <enum name="falls" value="2" />
        </attr>
        <attr name="horizontal_spacing" format="dimension" />
        <attr name="vertical_spacing" format="dimension" />
        <attr name="span_count" format="integer" />
        <attr name="horizontal_drawable" format="reference" />
        <attr name="vertical_drawable" format="reference" />
        <attr name="horizontal_color" format="color" />
        <attr name="vertical_color" format="color" />
        <attr name="header_footer_enable" format="boolean" />
        <attr name="header_footer_divider" format="boolean" />
        <attr name="bound_divider" format="boolean" />
        <attr name="item_anim" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DragView">
        <attr name="drag_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DragToRefreshBase">
        <attr name="drag_mode" format="enum">
            <enum name="disable" value="-1" />
            <enum name="never" value="0" />
            <enum name="both" value="1" />
            <enum name="start" value="2" />
            <enum name="end" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DragRefreshRecyclerView">
        <attr name="auto_load" format="boolean" />
        <attr name="sensitive_load" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DragRefreshLargeHeaderRecyclerView">
        <attr name="header_depth" format="dimension" />
        <attr name="header_drawable" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="SurfaceContainer">
        <attr name="aspectRatio" format="enum">
            <enum name="ratio_square" value="1" />
            <enum name="ratio_4x3" value="2" />
            <enum name="ratio_16x9" value="3" />
            <enum name="ratio_9x16" value="4" />
        </attr>
        <attr name="wrapper_type" format="enum">
            <enum name="holder" value="1" />
            <enum name="texture" value="2" />
            <enum name="ksy" value="3" />
        </attr>
        <attr name="surface_fill" format="boolean" />
        <attr name="fill_background" format="reference" />
    </declare-styleable>

    <declare-styleable name="PanelView">
        <attr name="ratio" format="enum">
            <enum name="ratio_square" value="1" />
            <enum name="ratio_4_3" value="2" />
            <enum name="ratio_16_9" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="GifView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomTheme">
        <attr name="gifViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="borderRadius" format="dimension" />
        <attr name="type">
            <enum name="circle" value="0" />
            <enum name="round" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="horizontalSpacing" format="dimension" />
        <attr name="verticalSpacing" format="dimension" />
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="debugDraw" format="boolean" />
    </declare-styleable>

    <declare-styleable name="FlowLayout_LayoutParams">
        <attr name="layout_newLine" format="boolean" />
        <attr name="layout_horizontalSpacing" format="dimension" />
        <attr name="layout_verticalSpacing" format="dimension" />
    </declare-styleable>

    <declare-styleable name="BannerView">
        <attr name="loop_enable" format="boolean" />
        <attr name="indicator_visible" format="boolean" />
        <attr name="indicator_drawable" format="reference" />
        <attr name="indicator_current_drawable" format="reference" />
        <attr name="indicator_gravity" format="enum">
            <enum name="top" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
            <enum name="hor_center" value="5" />
            <enum name="ver_center" value="6" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer" />
        <attr name="animDuration" format="integer" />
        <attr name="animAlphaStart" format="float" />
        <attr name="expandDrawable" format="reference" />
        <attr name="collapseDrawable" format="reference" />
        <attr name="expandText" format="string" />
        <attr name="collapseText" format="string" />
        <attr name="collapseExpandGravity" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="collapseExpandTextColor" format="color" />
        <attr name="collapseExpandTextSize" format="dimension" />
        <attr name="contentTextColor" format="color" />
        <attr name="contentBackground" format="reference" />
        <attr name="contentTextSize" format="dimension" />
        <attr name="textSelectable" format="boolean" />
        <attr name="justExpandable" format="boolean" />
    </declare-styleable>
    <eat-comment />
    <!-- 展开收起按钮 -->
    <item name="ui_expand_collapse" type="id" />
    <!-- 展开收起按钮图标 -->
    <item name="ui_expand_collapse_icon" type="id" />
    <!-- 文本控件 -->
    <item name="ui_expandable_text" type="id" />
    <!-- 展开收缩按钮容器 -->
    <item name="ui_expand_collapse_container" type="id" />

    <declare-styleable name="WheelView">
        <!-- 可见条目数量 -->
        <attr name="visible_count" format="integer" />
        <!-- 文字大小 -->
        <attr name="text_size" format="dimension" />
        <!-- 选中条目文字大小 -->
        <attr name="select_text_size" format="dimension" />
        <!-- 文字颜色 -->
        <attr name="text_color" format="color" />
        <!-- 选中条目文字颜色 -->
        <attr name="select_text_color" format="color" />
        <!-- 条目高度 -->
        <attr name="item_height" format="dimension" />
        <!-- 选中条目放大 -->
        <attr name="pop_style" format="boolean" />
        <!-- 指示器边框颜色 -->
        <attr name="indicator_border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="TitleBar">
        <attr name="title" format="string" />
        <attr name="backable" format="boolean" />
        <attr name="options" format="boolean" />
        <attr name="option_text" format="string" />
    </declare-styleable>

    <declare-styleable name="ShadowLayout">
        <attr name="sl_shadowed" format="boolean"/>
        <attr name="sl_shadow_distance" format="dimension"/>
        <attr name="sl_shadow_angle" format="integer"/>
        <attr name="sl_shadow_radius" format="dimension"/>
        <attr name="sl_shadow_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="CheckedView">
        <attr name="auto_checked" format="boolean" />
        <attr name="textOff" format="string" />
        <attr name="textOn" format="string" />
        <attr name="checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DotIndicator">
        <!-- 被选中圆点的颜色 -->
        <attr name="fillColor" format="color" />
        <!-- 未选中圆点的颜色 -->
        <attr name="strokeColor" format="color" />
        <!-- 圆点的大小 -->
        <attr name="dot_radius" format="dimension" />
        <!-- 圆点间间距的大小 -->
        <attr name="circleInterval" format="dimension" />
    </declare-styleable>

</resources>
